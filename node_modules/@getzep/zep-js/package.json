{
   "show-config": "tsc --showConfig",
   "name": "@getzep/zep-js",
   "version": "0.3.1",
   "description": "Zep JS Client",
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "main": "dist/index.js",
   "module": "dist/index.esm.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "lint": "eslint --ext .js,.ts src/",
      "build": "npm run lint && tsc && tsc --module ESNext --outDir dist/esm",
      "start": "node dist/index.js",
      "dev": "ts-node src/main.ts",
      "clean": "rm -rf dist/*",
      "prepublishOnly": "npm run clean && npm run build && npm run test && npm run docbuild",
      "prepare": "npm run build",
      "test": "jest --config jest.config.js",
      "test:watch": "jest --watch",
      "docbuild": "typedoc --out docs/ --excludePrivate --excludeInternal --excludeExternals src/"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/getzep/zep-js/"
   },
   "keywords": [
      "zep",
      "js"
   ],
   "author": "Sharath Rajasekar <sharathr@gmail.com>",
   "license": "Apache 2.0",
   "dependencies": {
      "axios": "^1.4.0",
      "typescript": "^5.0.4"
   },
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/jest": "^29.5.1",
      "@types/node": "^16.9.1",
      "@typescript-eslint/eslint-plugin": "^5.59.5",
      "@typescript-eslint/parser": "^5.59.5",
      "axios-mock-adapter": "^1.21.4",
      "dotenv": "^16.0.3",
      "eslint": "^8.40.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^29.5.0",
      "jsdoc": "^4.0.2",
      "prettier": "^2.8.8",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.2.1",
      "typedoc": "^0.24.7"
   },
   "files": [
      "dist"
   ],
   "jest": {
      "testMatch": [
         "**/__tests__/**/*.[jt]s?(x)",
         "**/?(*.)+(spec|test).[jt]s?(x)"
      ]
   }
}
